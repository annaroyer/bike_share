<%= form_for [:admin, @trip] do |f| %>
<div class="form-group row">
  <label class="col-sm-2 col-form-label">
    <%= f.label :duration %></label>
        <div class="col-sm-10">
    <%= f.number_field :duration %>
  </div>
</div>
<div class="form-group row">
  <label class="col-sm-2 col-form-label">
    <%= f.label :bike_id %></label>
  </div>
</div>
    <div class="col-sm-10">
  <%= f.number_field :bike_id %></div>
</div>
</div>

<label class="col-sm-2 col-form-label">
    <%= f.label :start_date %></label>
    <div class="col-sm-10">
    <%= f.date_field :start_date %>
  </div>
</div>

<div class="form-group row">
  <label class="col-sm-2 col-form-label">
    <%= f.label :start_station_id%></label>
    <div class="col-sm-10">
    <%= f.collection_select(:start_station_id, Station.order(:name), :id, :name) %>
  </div>
</div>

<label class="col-sm-2 col-form-label">
    <%= f.label :end_date %></label>
    <div class="col-sm-10">
    <%= f.date_field :end_date %>
  </div>
</div>

  <div class="form-group row">
    <label class="col-sm-2 col-form-label">
    <%= f.label :end_station_id %> </label>
    <div class="col-sm-10">
      <%= f.collection_select(:end_station_id, Station.order(:name), :id, :name)%></label>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :subscription_type %> </label>
        <div class="col-sm-10">
        <%= f.collection_select(:subscription_type, Trip.pluck(:subscription_type).uniq, :id, :subscription_type)%>
    </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
      <%= f.label :zip_code %></label>
      <div class="col-sm-10">
      <%= f.number_field :zip_code %>
    </div>
  </div>
    <%= f.submit %>
<% end %>
