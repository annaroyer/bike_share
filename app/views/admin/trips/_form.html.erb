<!DOCTYPE html>
<html>
  <head>
  </head>
    <body>
    </br>
    <%= form_for [:admin, @trip] do |f| %>
    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :duration %>
      <div class="col-sm-10">
        <%= f.number_field :duration %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :start_date %>
      <div class="col-sm-10">
        <%= f.date_field :start_date %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :start_station_id %>
      <div class="col-sm-10">
        <%= f.number_field :start_station_id %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :end_date %>
      <div class="col-sm-10">
        <%= f.date_field :end_date %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :end_station_id %>
      <div class="col-sm-10">
        <%= f.number_field :end_station_id %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :subscription_type %>
      <div class="col-sm-10">
        <%= f.text_field :subscription_type %>
      </div>
    </div>

    <div class="form-group row">
      <label class="col-sm-2 col-form-label">
        <%= f.label :zip_code %>
      <div class="col-sm-10">
        <%= f.number_field :zip_code %>
      </div>
    </div>

    <%= f.submit %>

  <% end %>
  </body>
</html>
