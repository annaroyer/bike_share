<h1 class="main-header"> Welcome, <%= current_user.full_name %> </h1>
<h2 class="sub-header"> Email: <%= current_user.email %></h2>
<h2 class="sub-header"> Account Created: <%= current_user.created_at %></h2>
<h2 class="sub-header"> Your Orders: </h2>
<% current_user.orders.each do |order| %>
  <%= link_to order.id, order_path(order) %>
<% end %>
<button type="button" class="btn"><%= link_to "Edit Account", edit_user_path(current_user) %></button>
